<?xml version="1.0" encoding="utf-8"?><LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"              android:orientation="horizontal"              android:layout_width="match_parent"              android:layout_height="match_parent">    <TextView        android:id="@+id/expList_date"        android:gravity="center_horizontal"        android:layout_weight="2.5"        android:layout_width="0dp"        android:layout_height="wrap_content"        android:text="날짜"/>    <TextView        android:id="@+id/expList_cardType"        android:gravity="center_horizontal"        android:layout_weight="2.5"        android:layout_width="0dp"        android:layout_height="wrap_content"        android:text="카드"/>    <TextView        android:id="@+id/expList_detail"        android:gravity="center_horizontal"        android:layout_weight="2.5"        android:layout_width="0dp"        android:layout_height="wrap_content"        android:text="상세"/>    <TextView        android:id="@+id/expList_price"        android:gravity="center_horizontal"        android:layout_weight="2.5"        android:layout_width="0dp"        android:layout_height="wrap_content"        android:text="지출"/></LinearLayout>